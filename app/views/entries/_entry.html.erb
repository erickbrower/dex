<article>
<h3><a href='#'><%= entry.title %></a></h3>
<h6><%= entry.etype.capitalize %> written by <a href='#'>cerickbrower@gmail.com</a> on <%= entry.created_at %></h6>
  <div class='row'>
    <div class='large-12 columns'>
      <% if entry.etype.downcase == 'markdown' %>
        <%= markdown_render(entry.content) %>
      <% elsif entry.etype.downcase != 'text' %>
        <%= raw source_code_render(entry.content, entry.etype) %>
      <% else %>
        <%= entry.content %>
      <% end %>
    </div>
  </div>
  <div class='row'>
    <div class='large-12 columns'>
    </div>
  </div>
</article>
<hr />
